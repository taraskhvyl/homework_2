footer.footer
    .container
        .footer__logo
            img(src="img/footer-logo.png", alt="Лого")
        .footer__navs
            .footer__navs_social
                ul.footer__navs_social-list
                    li.footer__navs_social-item
                        a(href="#" class="footer-facebook") Фейсбук
                            i.footer__icon-facebook
                    li.footer__navs_social-item
                        a(href="#" class="footer-vkontakte") Вконтакте
                            i.footer__icon-vkontakte
                    li.footer__navs_social-item
                        a(href="#" class="footer-twitter") Твиттер
                            i.footer__icon-twitter
            .footer__navs_menu
                -var footerNav = ['Планшеты', 'Мобильные телефоны', 'Mp3 плееры', 'Персональные компьютеры', 'Ноутбуки', 'Аксессуары', 'Бытовая техника'];
                ul.footer__navs_menu-list
                    each item in footerNav
                        li.footer__navs_menu-item
                            a(href="#" class="footer__navs_menu-url") #{item}
    .footer__line
